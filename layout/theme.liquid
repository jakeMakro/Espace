  </head>

  {%- capture tempClass -%}
    template-{{ template | replace: '.', ' ' | truncatewords: 1, '' | handle }}{% if template.suffix %} template-{{ template.suffix }}{% endif -%}
  {%- endcapture -%}
  
  <body class="gradient {{ tempClass }}{% if cart == empty %} cart-empty{% endif %}">
    <a class="skip-to-content-link button visually-hidden" href="#MainContent">
      {{ 'accessibility.skip_to_text' | t }}
    </a>

    <loading-bar class="loading-bar"></loading-bar>

    {%- if settings.cart_type == 'drawer' -%}
      {%- render 'cart-drawer' -%}
    {%- endif -%}
    {% sections 'header-group' %}

    <main id="MainContent" class="content-for-layout focus-none" role="main" tabindex="-1">
      {{ content_for_layout }}
    </main>

    {% sections 'footer-group' %}

    <ul hidden>
      <li id="a11y-refresh-page-message">{{ 'accessibility.refresh_page' | t }}</li>
      <li id="a11y-new-window-message">{{ 'accessibility.link_messages.new_window' | t }}</li>
    </ul>


    <script>
      window.shopUrl = '{{ request.origin }}';
      window.routes = {
        rootUrl: {{ routes.root_url | json }},
        rootUrlWithoutSlash: {% if routes.root_url == '/' %}''{% else %}{{ routes.root_url | json }}{% endif %},
        cart_add_url: '{{ routes.cart_add_url }}',
        cart_change_url: '{{ routes.cart_change_url }}',
        cart_update_url: '{{ routes.cart_update_url }}',
        cart_url: '{{ routes.cart_url }}',
        predictive_search_url: '{{ routes.predictive_search_url }}',
      };

      window.theme = {
        pageType: {{ request.page_type | json }},
        moneyFormat: {{ shop.money_format | json }},
        moneyWithCurrencyFormat: {{ shop.money_with_currency_format | json }}
      };

      window.cartStrings = {
        error: `{{ 'sections.cart.cart_error' | t }}`,
        quantityError: `{{ 'sections.cart.cart_quantity_error_html' | t: quantity: '[quantity]' }}`,
      };

      window.variantStrings = {
        addToCart: `{{ 'products.product.add_to_cart' | t }}`,
        soldOut: `{{ 'products.product.sold_out' | t }}`,
        unavailable: `{{ 'products.product.unavailable' | t }}`,
        unavailable_with_option: `{{ 'products.product.value_unavailable' | t: option_value: '[value]' }}`,
      };

      window.accessibilityStrings = {
        imageAvailable: `{{ 'products.product.media.image_available' | t: index: '[index]' }}`,
        shareSuccess: `{{ 'general.share.success_message' | t }}`,
        pauseSlideshow: `{{ 'sections.slideshow.pause_slideshow' | t }}`,
        playSlideshow: `{{ 'sections.slideshow.play_slideshow' | t }}`,
      };
    </script>

    {%- if settings.predictive_search_enabled -%}
      <script src="{{ 'predictive-search.js' | asset_url }}" defer="defer"></script>
    {%- endif -%}

    {% render 'swymSnippet' %}
    {% render 'swym-custom' %}

    {% if canonical_url == 'https://espace-67.com/en/pages/store-locator' or canonical_url == 'https://espace-67.com/pages/store-locator' %}
      {% render 'store-locator-script' %}
    {% endif %}

    <!-- Heyday start-->
    <script>
      window.hdAsyncInit = function() {
        _hdt.heyday_track_id('ClientIdAfterInit');
      }
    </script>
    {% if request.locale.iso_code == 'fr' %}
      <script async src='https://api.heyday.ai/v130/2932256625/309992382500714/fr/setupchat.js'></script>
    {% else %}
      <script async src='https://api.heyday.ai/v130/2932256625/309992382500714/en/setupchat.js'></script>    
    {% endif %}
    <!--Heyday end-->

    <script>
      {% if request.locale.iso_code == 'fr' %}
        /*Zapiet set fr for the fr language for checkout*/
        if(language_settings.translations){
          setTimeout(function(){
            language_settings.translations.pickup_language_code = 'fr';
            language_settings.translations.delivery_language_code = 'fr';
          },250);
        }
        /*Zapiet set fr for the fr language for checkout*/
      {% endif %}
    </script>
                                                                   
  </body>
</html>
{% endcapture %}
  
<!-- langify-v2-scripts-begin -->{% comment %}ly_liquid_scripts_begin{% endcomment %}
{% render 'ly-liquid-scripts' content: ly_liquid_content %}
{% comment %}ly_liquid_scripts_end{% endcomment %}
{% comment %}ly_core_scripts_begin{% endcomment %}
{% render 'ly-core-scripts' %}
{% comment %}ly_core_scripts_end{% endcomment %}
{% comment %}ly_switcher_factory_begin{% endcomment %}
{% render 'ly-switcher-factory' %}
{% comment %}ly_switcher_factory_end{% endcomment %}
