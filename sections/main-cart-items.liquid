{{ 'breadcrumb.css' | asset_url | stylesheet_tag }}
{{ 'component-cart.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'component-cart-items.css' | asset_url | stylesheet_tag: preload: true }}
{{ 'component-totals.css' | asset_url | stylesheet_tag }}
{{ 'component-price.css' | asset_url | stylesheet_tag }}
{{ 'component-discounts.css' | asset_url | stylesheet_tag }}
{% comment %}{{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}{% endcomment %}
{{ 'main-cart-items.css' | asset_url | stylesheet_tag: preload: true }}


{%- unless settings.cart_type == 'drawer' -%}
  <script src="{{ 'cart.js' | asset_url }}" defer="defer"></script>
{%- endunless -%}


{% comment %}** we remove protection plan product qty from main cart item count. because we didn't show protection plan product **{% endcomment %}

{%- assign warrantyProductCount = 0 -%}
{%- for item in cart.items -%}
  {%- if item.properties['Warranty_product'] != blank -%}
    {%- assign warrantyProductCount = warrantyProductCount | plus: item.quantity -%}
  {%- endif -%}
{%- endfor -%}
{%- assign cartCount = cart.item_count | minus: warrantyProductCount -%}

<cart-items class="page--width{% if cart == empty %} is-empty{% endif %}">

  {% render 'breadcrumb' %}

  <div class="title-wrapper-with-link">
    <h1 class="title title--primary">{{ 'sections.cart.title' | t }} <span data-cart-count>({{ cartCount }})</span></h1>
  </div>

  <div class="cart__warnings">
    <h1 class="cart__empty-text">{{ 'sections.cart.empty' | t }}</h1>
    <a href="{{ routes.all_products_collection_url }}" class="button primary-btn">
      {{ 'general.continue_shopping' | t }}
    </a>
    {%- if shop.customer_accounts_enabled and customer == null -%}
      <h2 class="cart__login-title">{{ 'sections.cart.login.title' | t }}</h2>
      <p class="cart__login-paragraph">
        {{ 'sections.cart.login.paragraph_html' | t: link: routes.account_login_url }}
      </p>
    {%- endif -%}
  </div>
  
  {%- assign brand = settings.hideBrand -%}

  <form action="{{ routes.cart_url }}" class="cart__contents critical-hidden" method="post" id="cart" autocomplete="off">
    <div class="cart__items" id="main-cart-items" data-id="{{ section.id }}">
      <div class="js-contents">
        {%- if cart != empty -%}

          <div class="cart-items-service">
            {%- for item in cart.items -%}

              {% assign protection_guide_link = blank %}
              
              {%- assign inStock = false -%}
              {%- assign isProtectionPlanSelected = false -%}
              {%- assign protectionPlanItem = blank -%}
              {%- include 'cart-product-tags' -%}
              
              {%- assign salesMultiple = item.product.metafields.pimData.salesMultiple | default: 1 | times: 1 -%}
              {% if salesMultiple <= 0 %}
                {%- assign salesMultiple = 1 -%}
              {% endif %}
              {%- assign hasSalesMultiple = false -%}
              {% if salesMultiple != blank and salesMultiple > 1 %}
                {%- assign hasSalesMultiple = true -%}
              {% endif %}

              {%- assign selectedProtectionPlanElement = blank -%}
              {%- assign protectionPlanElement = blank -%}
              
              <div class="cart-service-row{% if isProtectionPlanSelected %} has-protection-plan-added{% endif %}{% if item.properties['Warranty_product'] != blank %} protection-plan-product hidden{% endif%}" 
                  data-item-key="{{ item.key }}"
                  data-timestamp="{% if item.properties['_timestamp'] != blank %}{{ item.properties._timestamp }}{% endif%}"
                  data-props='{{ item.properties | json }}'
                  data-qty='{{ item.quantity }}'
              >
                
                <div class="cart-item__media">
                  {%- if inStock -%}
                    <div class="cart-stock-tag small-hide">
                      <span>{{ 'sections.cart.in_stock_label' | t }}</span>
                    </div>
                  {% endif %}
              
                  {% if item.image %}
                    <a href="{{ item.url }}" class="cart-item__image-container gradient global-media-settings">
                      <img
                              src="{{ item.image | image_url: width: 300 }}"
                              class="cart-item__image"
                              alt="{{ item.image.alt | escape }}"
                              loading="{% if forloop.index > 2 %}lazy{% else %}eager{% endif %}"
                              width="150"
                              height="{{ 150 | divided_by: item.image.aspect_ratio | ceil }}"
                            >
                    </a>
                  {% endif %}  
                </div>

                <div class="cart-service-right">
              
                  <div class="cart-service-head">
                    <div class="col-left">

                      {%- if item.variant.compare_at_price > item.variant.price -%}
                        <div class="cart-service-tag">
                          <div class="tag-box">En vente</div>
                        </div>
                      {%- endif -%}
              
                      <a href="{{ item.url }}" class="cart-item__name h4 break">
                        {%- if item.product.vendor != blank %}
                        {%- unless brand contains item.product.vendor -%}
                          <strong>{{ item.product.vendor }}</strong>{%- endunless -%}{%- endif %}{{ item.product.title | escape -}}
                      </a>
                      <p class="product-option cart-service-variant hidden">White</p>
              
                      {%- if item.product.has_only_default_variant == false
                        or item.properties.size != 0
                        or item.selling_plan_allocation != null
                      -%}
                        <dl>
                          {%- if item.product.has_only_default_variant == false -%}
                            {%- for option in item.options_with_values -%}
                              <div class="product-option cart-service-variant">
                                <dt>{{ option.name }}:</dt>
                                <dd>{{ option.value }}</dd>
                              </div>
                            {%- endfor -%}
                          {%- endif -%}
  
                          {%- for property in item.properties -%}
                            {%- assign property_first_char = property.first | slice: 0 -%}
                            {%- if property.last != blank and property_first_char != '_' and property.first != 'Sales Multiple' -%}
                              <div class="product-option cart-service-variant">
                                <dt>{{ property.first }}:</dt>
                                <dd>
                                  {%- if property.last contains '/uploads/' -%}
                                    <a href="{{ property.last }}" class="link" target="_blank">
                                      {{ property.last | split: '/' | last }}
                                    </a>
                                  {%- else -%}
                                    {{ property.last }}
                                  {%- endif -%}
                                </dd>
                              </div>
                            {%- endif -%}
                          {%- endfor -%}
                        </dl>
                        
                        <p class="product-option fairstone-product-cart-calculator" data-price="{{ line_item.original_line_price }}"></p>
  
                        {% if item.selling_plan_allocation.selling_plan.name %}
                          <p class="product-option">{{ item.selling_plan_allocation.selling_plan.name }}</p>
                        {% endif %}
                        
                      {%- endif -%}
              
                      <div class="cart-service-price small-hide">
                        {%- capture cartPrice -%}
                        <div class="price price--on-sale">
                          <div class="price__container">
                            {%- if item.original_price != item.final_price -%}
                              <div class="price__regular hidden">
                                <span class="price-item price-item--regular">
                                  <span class="money">{{- item.original_price | times: item.quantity | money -}}</span>
                                </span>
                              </div>
                            {% else %}
                              <div class="price__sale">
                                {% if item.variant.compare_at_price > item.variant.price %}
                                  <s class="price-item price-item--regular">
                                    <span class="money">{{ item.variant.compare_at_price | times: item.quantity | money }}</span>
                                  </s>
                                {% endif %}
                                <span class="price-item price-item--sale price-item--last">
                                  <span class="money">{{ item.original_price | times: item.quantity | money }}</span>
                                </span>
                              </div>
                            {% endif %}
                          </div>
                        </div>
                        {%- endcapture -%}  
                        {{- cartPrice -}}
                      </div>
              
                    </div>
              
                    <div class="col-right small-hide">
                      {%- capture quantityWrapper -%}
                      <div class="cart-item__quantity-wrapper">
                        <quantity-input class="quantity cart-quantity">
                          <button class="quantity__button no-js-hidden" name="minus" type="button">
                            {%- render 'icon-minus' -%}
                          </button>
                          {% assign quantity_min = item.variant.quantity_rule.min %}
                          {% assign quantity_step = item.variant.quantity_rule.increment %}
                          {% if hasSalesMultiple %}
                            {% assign quantity_min = salesMultiple %}
                            {% assign quantity_step = salesMultiple %}
                          {% endif %}
                          <input
                            class="quantity__input"
                            data-quantity-variant-id="{{ item.variant.id }}"
                            type="number"
                            name="updates[]"
                            value="{{ item.quantity }}"
                            {% # theme-check-disable %}
                            data-cart-quantity="{{ cart | item_count_for_variant: item.variant.id }}"
                            min="{{ quantity_min }}"
                            {% if item.variant.quantity_rule.max != null %}
                              max="{{ item.variant.quantity_rule.max }}"
                            {% endif %}
                            step="{{ quantity_step }}"
                            {% # theme-check-enable %}
                            aria-label="{{ 'products.product.quantity.input_label' | t: product: item.product.title | escape }}"
                            id="Quantity-{{ item.index | plus: 1 }}"
                            data-index="{{ item.index | plus: 1 }}"
                          >
                          <button class="quantity__button no-js-hidden" name="plus" type="button">
                            {%- render 'icon-plus' -%}
                          </button>
                        </quantity-input>
              
                        {%- if isProtectionPlanSelected -%}
                          {%- capture removeTag -%}multiple-item-remove-button{%- endcapture -%}
                        {%- else -%}
                          {%- capture removeTag -%}cart-remove-button{%- endcapture -%}
                        {%- endif -%}
              
                        <{{ removeTag }}
                          id="Remove-{{ item.index | plus: 1 }}"
                          data-index="{{ item.index | plus: 1 }}"
                          data-key="{{ item.key }}"
                        >
                          <a
                            href="{{ item.url_to_remove }}"
                            class="button button--tertiary"
                            aria-label="{{ 'sections.cart.remove_title' | t: title: item.title }}"
                          >
                            {%- render 'icon-remove' -%}
                          </a>
                        </{{ removeTag }}>
                      </div>
                      {%- endcapture -%}
                      {{- quantityWrapper -}}
                    </div>
                  </div>

                  <div class="cart-service-info small-hide">

                    {%- if isProtectionPlanSelected -%}
                      {%- capture selectedProtectionPlan -%}
                      <div class="cart-anti-plan selected-protection-plan">
                        <div class="cart-anti-text">
                          <span class="cart-anti-title">{{- protectionPlanItem.product.title | escape -}}</span>
                          <span class="cart-anti-price">{{ protectionPlanItem.line_price | money }}</span>
                          <protection-plan class="tooltip applince-tooltip">
                            <a href="{{protection_guide_link}}" data-protection-content>
                              {%- render 'icon-tooltip' -%}
                            </a>
                          </protection-plan>
                        </div>
                        <protection-plan-remove-button
                            id="Remove-{{ protectionPlanItem.index | plus: 1 }}"
                            data-index="{{ protectionPlanItem.index | plus: 1 }}"
                            data-key="{{ protectionPlanItem.key }}"
                          >
                            <a
                              href="{{ protectionPlanItem.url_to_remove }}"
                              class="button--tertiary"
                              aria-label="{{ 'sections.cart.remove_title' | t: title: protectionPlanItem.title }}"
                            >
                              {% render 'icon-remove' %}
                            </a>
                        </protection-plan-remove-button>
                      </div>
                      {%- endcapture -%}
                      {%- assign selectedProtectionPlanElement = selectedProtectionPlan -%}
                      {{- selectedProtectionPlanElement -}}
                    {%- endif -%}

                    {%- capture cartProtectionPlan -%}
                    {%- render "cart-protection-plan", line_item: item, isProtectionPlanSelected: isProtectionPlanSelected -%}
                    {%- endcapture -%}

                    {%- assign protectionPlanElement = cartProtectionPlan -%}

                    {{- protectionPlanElement -}}
  
                  </div>
                  
                  
                </div>


                <div class="cart-service-info medium-hide large-up-hide">
  
                  <div class="price-qty-bar">
                    <div class="cart-service-price">
                      {{- cartPrice -}}
                      {%- if inStock -%}
                        <div class="cart-stock-tag">
                          <span>{{ 'sections.cart.in_stock_label' | t }}</span>
                        </div>
                      {% endif %}
                    </div>
                    {{- quantityWrapper -}}
                  </div>

                  {%- if selectedProtectionPlanElement != blank -%}
                    <div class="cart-service-appliance">
                      {{- selectedProtectionPlanElement -}}
                      {{- protectionPlanElement -}}
                    </div>
                  {%- else -%}
                    {{- selectedProtectionPlanElement -}}
                    {{- protectionPlanElement -}}
                  {%- endif -%}
  
                </div>
                          
                
              </div>
            
            {%- endfor -%}
          </div>
        
        {%- endif -%}
      </div>
    </div>

    <p class="visually-hidden" id="cart-live-region-text" aria-live="polite" role="status"></p>
    <p class="visually-hidden" id="shopping-cart-line-item-status" aria-live="polite" aria-hidden="true" role="status">
      {{ 'accessibility.loading' | t }}
    </p>
    
  </form>
  
</cart-items>




{% comment %}
<cart-items class="page--width{% if cart == empty %} is-empty{% endif %}">

  {% render 'breadcrumb' %}
  
  <div class="title-wrapper-with-link">
    <h1 class="title title--primary">{{ 'sections.cart.title' | t }} <span>(4)</span></h1>
    {% comment %}<a href="{{ routes.all_products_collection_url }}" class="underlined-link">{{ 'general.continue_shopping' | t }}</a>{% endcomment %}
  </div>

  <div class="cart__warnings">
    <h1 class="cart__empty-text">{{ 'sections.cart.empty' | t }}</h1>
    <a href="{{ routes.all_products_collection_url }}" class="button">
      {{ 'general.continue_shopping' | t }}
    </a>

    {%- if shop.customer_accounts_enabled and customer == null -%}
      <h2 class="cart__login-title">{{ 'sections.cart.login.title' | t }}</h2>
      <p class="cart__login-paragraph">
        {{ 'sections.cart.login.paragraph_html' | t: link: routes.account_login_url }}
      </p>
    {%- endif -%}
  </div>

  <form action="{{ routes.cart_url }}" class="cart__contents critical-hidden" method="post" id="cart">
    <div class="cart__items" id="main-cart-items" data-id="{{ section.id }}">
      <div class="js-contents">
        {%- if cart != empty -%}
        {% comment %}
          <table class="cart-items">
            <caption class="visually-hidden">
              {{ 'sections.cart.title' | t }}
            </caption>
            <thead>
              <tr>
                <th class="caption-with-letter-spacing" colspan="2" scope="col">
                  {{ 'sections.cart.headings.product' | t }}
                </th>
                <th class="medium-hide large-up-hide right caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.total' | t }}
                </th>
                <th class="cart-items__heading--wide small-hide caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.quantity' | t }}
                </th>
                <th class="small-hide right caption-with-letter-spacing" colspan="1" scope="col">
                  {{ 'sections.cart.headings.total' | t }}
                </th>
              </tr>
            </thead>

            <tbody>
              {%- for item in cart.items -%}
                {% assign display_section = false %}
                {% assign show_assembly_service = false %}
                
                {% assign tags_prop = item.properties._tags %}
                {% assign warrenty_prop = item.properties.Warranty_product %}
                {% assign shop_field_tags = shop.metafields.tags.appliance_plan %}
                {% assign preorder_state = item.properties.precommande %}
                {% if shop.metafields.tags.antitache_plan != blank %}
                    {% assign shop_field_tags = shop_field_tags | append:'|' | append:shop.metafields.tags.antitache_plan %}
                {% endif %}
               
                <span class="hide" value="{{shop_field_tags}}"></span>
                {% if shop_field_tags contains '|' %}
                    
                      {% assign shop_field_tags_split = shop_field_tags | split:'|' %}
                      {% assign tags_prop_split = item.properties._tags | split:',' %}
                  
                      {% for tags_name in tags_prop_split %}
                            {% for tags in shop_field_tags_split %}
                              
                {% if tags == tags_name %}
                            {% assign display_section = true %}
                          {% endif %}
                            {% endfor %}
                      {% endfor %}
                    {% else %}
                      {% if tags_prop contains shop_field_tags %}
                        {% assign display_section = true %}
                      {% endif %}
                    {% endif %}
                    
                      {% assign tags_prop_line = item.properties._tags %}
                      {% if tags_prop_line contains ',' %}
                        {% assign tags_prop_line = tags_prop_line | split:',' %}
                      {% endif %}

                      {% assign applince_tags_line = shop.metafields.tags.appliance_plan %}
                      {% if applince_tags_line contains '|' %}
                        {% assign applince_tags_line = shop.metafields.tags.appliance_plan | split:'|' %}
                      {% endif %}

                      {% assign antitache_tags_line = shop.metafields.tags.antitache_plan %}
                      {% if antitache_tags_line contains '|' %}
                        {% assign antitache_tags_line = shop.metafields.tags.antitache_plan | split:'|' %}
                      {% endif %}


                      {% assign applince_product_line = false %}
                      {% assign antitache_product_line = false %}

                      {% if item.properties._tags contains ',' %}
                        {% for tag_loop_applince_line in tags_prop_line %}
                          {% if shop.metafields.tags.appliance_plan contains '|' %}
                            {% for applince_loop_line in applince_tags_line %}
                              {% if tag_loop_applince_line == applince_loop_line %}
                                {% assign applince_product_line = true %}
                              {% endif %}
                            {% endfor %}
                          {% else %}
                            {% if tag_loop_applince_line == applince_tags_line %}
                              {% assign applince_product_line = true %}
                            {% endif %}
                          {% endif %}
                        {% endfor %}

                        {% for tag_loop_antitache_line in tags_prop_line %}
                          {% if shop.metafields.tags.antitache_plan contains '|' %}
                            {% for antitache_loop_line in antitache_tags_line %}
                              {% if tag_loop_antitache_line == antitache_loop_line %}
                                {% assign antitache_product_line = true %}
                              {% endif %}
                            {% endfor %}
                          {% else %}
                            {% if tag_loop_antitache_line == antitache_tags_line %}
                              {% assign antitache_product_line = true %}
                            {% endif %}
                          {% endif %}
                        {% endfor %}
                      {% else %}
                          {% if shop.metafields.tags.appliance_plan contains '|' %}
                            {% for applince_loop_line in applince_tags_line %}
                              {% if tags_prop_line == applince_loop_line %}
                                {% assign applince_product_line = true %}
                              {% endif %}
                            {% endfor %}
                          {% else %}
                            {% if tags_prop_line == applince_tags_line %}
                              {% assign applince_product_line = true %}
                            {% endif %}
                          {% endif %}
                        
                          {% if shop.metafields.tags.antitache_plan contains '|' %}
                            {% for antitache_loop_line in antitache_tags_line %}
                              {% if tags_prop_line == antitache_loop_line %}
                                {% assign antitache_product_line = true %}
                              {% endif %}
                            {% endfor %}
                          {% else %}
                            {% if tags_prop_line == antitache_tags_line %}
                              {% assign antitache_product_line = true %}
                            {% endif %}
                          {% endif %}
                      {% endif %}
                 <span class="hide class" data-tag-val="{{applince_product_line}}-{{antitache_product_line}}--{{display_section}} "></span>
                <tr id="CartItem-{{ item.index | plus: 1 }}" data-ptr="{{ item.properties.PTR }}" data-handle="{{ item.product.handle }}" data-key="{{ item.key }}" class="cart-item {{ item.properties.Warranty_product }}-- line-item line-item--stack {% if warrenty_prop == 'true' %}warrenty-product{% endif %} {% if display_section == true %}{% if applince_product_line == true %}appliance-warrenty{% elsif antitache_product_line == true %}furniture-warrenty{% endif %}{% endif %}" {% if item.properties.Warranty_product %}style="display: none;"{% endif%} data-id="{{item.variant_id}}" data-timestamp="{% if item.properties.timestamp %}{{item.properties.timestamp}}{% endif%}" data-index="{{item.index | plus:1 }}" data-qty="{{ item.quantity }}" data-tag="{{item.properties._tags}}" data-preorder="{% if preorder_state %}true{% else %}false{% endif %}" data-warrenty="{% if item.properties.Garantie_ajoutee %}true{% else %}false{% endif %}" data-pick="{% if item.properties.Ramassage %}true{% else %}false{% endif %}">
                  <td class="cart-item__media">
                    <div class="cart-stock-tag">
                      <span>In Stock</span>
                    </div>
                    {% if item.image %}
                      {% comment %} Leave empty space due to a:empty CSS display: none rule {% endcomment %}
                      {% comment %}<a href="{{ item.url }}" class="cart-item__link" aria-hidden="true" tabindex="-1"></a>{% endcomment %}
                      <a href="{{ item.url }}" aria-hidden="true" tabindex="-1" class="cart-item__image-container gradient global-media-settings">
                        <img
                          src="{{ item.image | image_url: width: 300 }}"
                          class="cart-item__image"
                          alt="{{ item.image.alt | escape }}"
                          loading="lazy"
                          width="150"
                          height="{{ 150 | divided_by: item.image.aspect_ratio | ceil }}"
                        >
                      </a>
                    {% endif %}
                  </td>

                  <td class="cart-item__details">
                    {%- if settings.show_vendor -%}
                      <p class="caption-with-letter-spacing">{{ item.product.vendor }}</p>
                    {%- endif -%}
                    {% assign shipping_method = false %} 
                    {% for tag in item.product.tags %}
                      {% if tag == "Free" %}
                      {% assign shipping_method = true %}  
                      {% endif %}
                    {% endfor %}
                    {% if shipping_method == true %}
                    <span class="freeship-tag">free shipping</span>  
                    {% endif %}
                    {% if item.variant.compare_at_price > item.variant.price %}
                      <span class="if-compare">Clearance</span>
                    {% endif %}
                    <a href="{{ item.url }}" class="cart-item__name h4 break">{{ item.product.title | escape }}</a>
                    {%- if item.original_price != item.final_price -%}
                      <div class="cart-item__discounted-prices">
                        <span class="visually-hidden">
                          {{ 'products.product.price.regular_price' | t }}
                        </span>
                        <s class="cart-item__old-price product-option">
                          {{- item.original_price | money -}}
                        </s>
                        <span class="visually-hidden">
                          {{ 'products.product.price.sale_price' | t }}
                        </span>
                        <strong class="cart-item__final-price product-option">
                         {{ item.final_price | money }}
                        </strong>
                      </div>
                    {%- else -%}
                      <div class="product-option">
                       {{ item.original_price | money }}
                      {% if item.variant.compare_at_price > item.variant.price %}
                        <s>{{ item.variant.compare_at_price | money }}</s>
                      {% assign savings = item.variant.compare_at_price | minus: item.final_price %}
                      <span>% Clearance</span> -{{ savings | money }}                        
                      {% endif %}

                      </div>
                    {%- endif -%}

                    {%- if item.product.has_only_default_variant == false
                      or item.properties.size != 0
                      or item.selling_plan_allocation != null
                    -%}
                      <dl>
                        {%- if item.product.has_only_default_variant == false -%}
                          {%- for option in item.options_with_values -%}
                            <div class="product-option">
                              <dt>{{ option.name }}:</dt>
                              <dd>{{ option.value }}</dd>
                            </div>
                          {%- endfor -%}
                        {%- endif -%}

                        {%- for property in item.properties -%}
                          {%- assign property_first_char = property.first | slice: 0 -%}
                          {%- if property.last != blank and property_first_char != '_' -%}
                            <div class="product-option">
                              <dt>{{ property.first }}:</dt>
                              <dd>
                                {%- if property.last contains '/uploads/' -%}
                                  <a href="{{ property.last }}" class="link" target="_blank">
                                    {{ property.last | split: '/' | last }}
                                  </a>
                                {%- else -%}
                                  {{ property.last }}
                                {%- endif -%}
                              </dd>
                            </div>
                          {%- endif -%}
                        {%- endfor -%}
                      </dl>

                      <p class="product-option">{{ item.selling_plan_allocation.selling_plan.name }}</p>
                    {%- endif -%}

                    <ul class="discounts list-unstyled" role="list" aria-label="{{ 'customer.order.discount' | t }}">
                      {%- for discount in item.discounts -%}
                        <li class="discounts__discount">
                          {%- render 'icon-discount' -%}
                          {{ discount.title }}
                        </li>
                      {%- endfor -%}
                    </ul>
                  </td>

                  <td class="cart-item__totals right medium-hide large-up-hide">
                    <div class="loading-overlay hidden">
                      <div class="loading-overlay__spinner">
                        <svg
                          aria-hidden="true"
                          focusable="false"
                          class="spinner"
                          viewBox="0 0 66 66"
                          xmlns="http://www.w3.org/2000/svg"
                        >
                          <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
                        </svg>
