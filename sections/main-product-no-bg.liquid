<script>
  window.addEventListener('load', function(event){
    //affrim price product page
    var fairStonePriceSetup;
    setTimeout(function(){
      if( $('.fairstone-product-calculator').length > 0 ){
        fairStonePriceSetup = setInterval(fairStoneSetup, 1000);
      }
    },1600);
    
    function fairStoneSetup(){
      $('body:not([data-fairStoneSetup])').attr('data-fairStoneSetup',0);
    	if($('.fairstone-product-calculator').children().length > 0) {
        let fraiStonePrice, fraiStoneMonth, firstLine, fsTTip = '';
        $('.fairstone-product-calculator').children().each(function(index, ele){
          if( index == 0 ){    
            $(ele).children().each(function(childIndex, childEle){            
              let text = $(childEle).text().trim();
              if( childIndex == 0 ){
                firstLine = text;
              }
              if( $(childEle).hasClass('price') ){
                text = text.split('/')[0];
                fraiStonePrice = text.trim();
              }else{
                if( text.match(/months/) != null || text.match(/mois/) != null ){
                  text = text.split(' ')[0];
                  fraiStoneMonth = text.trim();
                }                
              }
            });
          }else{
            if( $(ele).find('a') != null ){
              fsTTip = `<a href="${ $(ele).find('a').attr('href') }" title="">${ $(ele).find('a').text().length ? $(ele).find('a').text().trim(): 'Apply Now' }</a>`;
            }
          }
        });
        if( fraiStonePrice != null && fraiStoneMonth != null ){
          var fsHTMl = `<div class="fsCustomCon">${ fairStoneIcon } ${ firstLine } <strong class="price">${ fraiStonePrice }<span>/mo</span></strong> for <span class="month_text">${fraiStoneMonth} months</span> ${ fsTTip != '' ? (' '+fsTTip):'' }</div>`;
          $('.fairS-pro-cal .fairstone-product-calculator').children().addClass('hidden');        
          $('.fairS-pro-cal .fairstone-product-calculator').append(fsHTMl);
          $('.fairS-pro-cal .fairstone-product-calculator').removeClass('hidden');
        }
        $('body').removeAttr('data-fairStoneSetup',0);
        clearInterval(fairStonePriceSetup);
      }else{
        var countfairS = Number($('body').attr('data-fairStoneSetup'));
        if( countfairS > 10 ){
          clearInterval(fairStonePriceSetup);
          $('.fairS-pro-cal .fairstone-product-calculator').not('.hidden').addClass('hidden');        
        }
      }
    }
    //end
  });
</script>
{% endif %}

