<section
  id="MainProduct-{{ section.id }}"
  class="page-width section-{{ section.id }}-padding no-bg"
  data-section="{{ section.id }}"
>
  {{ 'section-main-product.css' | asset_url | stylesheet_tag: preload: true }}
  {{ 'component-accordion.css' | asset_url | stylesheet_tag }}
  {{ 'component-price.css' | asset_url | stylesheet_tag }}
  {{ 'component-slider.css' | asset_url | stylesheet_tag }}
  {{ 'component-rating.css' | asset_url | stylesheet_tag }}
  {{ 'component-loading-overlay.css' | asset_url | stylesheet_tag }}
  {{ 'component-deferred-media.css' | asset_url | stylesheet_tag }}

  {{ 'main-product.css' | asset_url | stylesheet_tag: preload: true }}

  {%- style -%}
    .section-{{ section.id }}-padding {
      padding-top: {{ section.settings.padding_top | times: 0.75 | round: 0 }}px;
      padding-bottom: {{ section.settings.padding_bottom | times: 0.75 | round: 0 }}px;
    }

    @media screen and (min-width: 750px) {
      .section-{{ section.id }}-padding {
        padding-top: {{ section.settings.padding_top }}px;
        padding-bottom: {{ section.settings.padding_bottom }}px;
      }
    }
  {%- endstyle -%}

  <script src="{{ 'product-info.js' | asset_url }}" defer="defer"></script>
  <script src="{{ 'product-form.js' | asset_url }}" defer="defer"></script>
  
  {% if section.settings.image_zoom == 'hover' %}
    <script id="EnableZoomOnHover-main" src="{{ 'magnify.js' | asset_url }}" defer="defer"></script>
  {% endif %}
  {%- if request.design_mode -%}
    <script src="{{ 'theme-editor.js' | asset_url }}" defer="defer"></script>
  {%- endif -%}

  {%- assign first_3d_model = product.media | where: 'media_type', 'model' | first -%}
  {%- if first_3d_model -%}
    {{ 'component-product-model.css' | asset_url | stylesheet_tag }}
    <link
      id="ModelViewerStyle"
      rel="stylesheet"
      href="https://cdn.shopify.com/shopifycloud/model-viewer-ui/assets/v1.0/model-viewer-ui.css"
      media="print"
      onload="this.media='all'"
    >
    <link
      id="ModelViewerOverride"
      rel="stylesheet"
      href="{{ 'component-model-viewer-ui.css' | asset_url }}"
      media="print"
      onload="this.media='all'"
    >
  {%- endif -%}

  {% assign variant_images = product.images | where: 'attached_to_variant?', true | map: 'src' %}

  {%- assign salesMultiple = product.metafields.pimData.salesMultiple | default: 1 | times: 1 -%}  
  {% if salesMultiple <= 0 %}
    {%- assign salesMultiple = 1 -%}
  {% endif %}  
  {%- assign hasSalesMultiple = false -%}  
  {% if salesMultiple != blank and salesMultiple > 1 %}
    {%- assign hasSalesMultiple = true -%}
  {% endif %}
  
  {% capture quantity_selector %}
  <div
    id="Quantity-Form-{{ section.id }}"
    class="product-form__input product-form__quantity{% if settings.inputs_shadow_vertical_offset != 0 and settings.inputs_shadow_vertical_offset < 0 %} product-form__quantity-top{% endif %}"
    {{ block.shopify_attributes }}
  >
    {% comment %} TODO: enable theme-check once `item_count_for_variant` is accepted as valid filter {% endcomment %}
    {% # theme-check-disable %}
    {%- assign cart_qty = cart | item_count_for_variant: product.selected_or_first_available_variant.id -%}
    {% # theme-check-enable %}
    {% assign quantity_step = product.selected_or_first_available_variant.quantity_rule.increment %}
    {% assign quantity_min = product.selected_or_first_available_variant.quantity_rule.min %}
    {% if hasSalesMultiple %}
      {% assign quantity_step = salesMultiple %}
      {% assign quantity_min = salesMultiple %}
    {% endif %}
    
    <label class="quantity__label form__label" for="Quantity-{{ section.id }}">
      {{ 'products.product.quantity.label' | t }}
      <span class="quantity__rules-cart no-js-hidden{% if cart_qty == 0 %} hidden{% endif %}">
        <span class="loading-overlay hidden">
          <span class="loading-overlay__spinner">
            <svg
              aria-hidden="true"
              focusable="false"
              class="spinner"
              viewBox="0 0 66 66"
              xmlns="http://www.w3.org/2000/svg"
            >
              <circle class="path" fill="none" stroke-width="6" cx="33" cy="33" r="30"></circle>
            </svg>
          </span>
        </span>
        <span>({{- 'products.product.quantity.in_cart_html' | t: quantity: cart_qty -}})</span>
      </span>
    </label>
    <quantity-input class="quantity">
      <button class="quantity__button no-js-hidden" name="minus" type="button">
        <span class="visually-hidden">
          {{- 'products.product.quantity.decrease' | t: product: product.title | escape -}}
        </span>
        {% render 'icon-minus' %}
      </button>
      <input
        class="quantity__input"
        type="number"
        name="quantity"
        id="Quantity-{{ section.id }}"
        data-cart-quantity="{{ cart_qty }}"
        data-min="{{ quantity_min }}"
        min="{{ quantity_min }}"
        {% if product.selected_or_first_available_variant.quantity_rule.max != null %}
          data-max="{{ product.selected_or_first_available_variant.quantity_rule.max }}"
          max="{{ product.selected_or_first_available_variant.quantity_rule.max }}"
        {% endif %}
        step="{{ quantity_step }}"
        value="{{ quantity_min }}"
        form="{{ product_form_id }}"
      />
      <button class="quantity__button no-js-hidden" name="plus" type="button">
        <span class="visually-hidden">
          {{- 'products.product.quantity.increase' | t: product: product.title | escape -}}
        </span>
        {% render 'icon-plus' %}
      </button>
    </quantity-input>
    <div class="quantity__rules caption no-js-hidden">
      {%- if product.selected_or_first_available_variant.quantity_rule.increment > 1 -%}
        <span class="divider">
          {{-
            'products.product.quantity.multiples_of'
            | t: quantity: product.selected_or_first_available_variant.quantity_rule.increment
          -}}
        </span>
      {%- endif -%}
      {%- if product.selected_or_first_available_variant.quantity_rule.min > 1 -%}
        <span class="divider">
          {{-
            'products.product.quantity.minimum_of'
            | t: quantity: product.selected_or_first_available_variant.quantity_rule.min
          -}}
        </span>
      {%- endif -%}
      {%- if product.selected_or_first_available_variant.quantity_rule.max != null -%}
        <span class="divider">
          {{-
            'products.product.quantity.maximum_of'
            | t: quantity: product.selected_or_first_available_variant.quantity_rule.max
          -}}
        </span>
      {%- endif -%}
    </div>
  </div>
  {% endcapture %}{% assign quantity_selector = quantity_selector | strip %}
    
  <div class="product product--{{ section.settings.media_size }} product--{{ section.settings.media_position }} product--{{ section.settings.gallery_layout }} product--mobile-hide grid grid--1-col {% if product.media.size > 0 %}grid--2-col-tablet{% else %}product--no-media-custom{% endif %}">
    <div class="mob-title product__info-container">
      <div class="vendor-tag-model">
        {% assign displaymodel = false %}
        {% for tag in product.tags %}
          {% if tag contains 'Model_' %}
            {% assign displaymodel = true %}
            {% assign tag_data = tag | split: 'Model_' %}
          {% break %}
          {% endif %}
        {% endfor %}  
        {% assign Instock = false %}
        {% for tag in product.tags %}
          {% if tag contains 'In Stock_Yes' %}
            {% assign Instock = true %}
          {% break %}
          {% endif %}
        {% endfor %}

      {%- assign hideBrand = true -%}
      {%- assign brand = "Donald_choi,Collection Espace 67,Amalfi,Brassex,Canadian Sofa Solutions,Dormatex,EK Design,Furnishings Mate,JMG Global,Korson Furniture,Libra,Mazin Furniture,Universal,Agence Viva,Aico,ALF,Arteriors Home,Lyon Capital,B2mod,Bassett,Tapis Beaver,BDI,Bellini,Bernhardt,Calli,Canfloyd,Celadon,Chintaly Imports,Couture Jardin,Cuddle Down,David Youngson & Associes,Decor-Rest,Durham Furniture,Eastern Accents,EK Design,Ekornes,Elan,Elk Lightning,Elran,Flow Decor,Forge Design,Four Hands,Furnishings Mate,Galaxy Bedding,Hammary,Hinkley Lighting,Hooker Furniture,Howard Miller,Huppé,Img,Incanto Italia,International Furniture Direct,Jaymar,Meubles JLM,Jmg Global,Kalora,Kaza Design,Kendal Lighting,Claire Scott & ass.,Korson Furniture,Kuka Home,Kuka Motion,Kuka Home,Legacy Classic,Lexington,Lind Canada,Livia,Luce Lumen,La-Z-Boy,Magnussen,Milky Way Lighting,Mobican,Mobital,Moe's Home Collection,Monarch Specialities,Murray Feiss,Natart Juvenile,Natuzzi Italia,Natuzzi Editions,Nca Design,Norlite,Meubles Nouveau Concept,Nuevo,Owen,Pacific Coast Lighting,Palliser,The Phillips Collection,Le Present,Progress Lighting,Home Meridian International,Regal Reflections,Regina Andrew,Renwil,Rjs Trading,Surya,Sagebrook Home,Vivet,Stevens Omni,Studio A,Sua-V,Sunpan,Technogel,Tempur,Tomali,Torre & Tagus,Tuff Avenue,Uttermost,Vertuu Design,Verbois,Via Meubles,Viebois,Violino,Wac Lighting" -%}      
      {% unless brand contains product.vendor %}
        {%- assign hideBrand = false -%}
        <div class="vendor-custom">{{ product.vendor }}</div>
      {% endunless %}
      
      {% comment %}
      {% if displaymodel == true %}
        <div class="model-tag-custom">
           <label>Model:</label>
           <span id="model_number">{{ tag_data[1] }}</span>
           <span class="copy-btn" data-type="attribute" data-attr-name="data-clipboard-text" data-clipboard-text="{{ tag_data[1] }}">{% render 'icon-copy' %}</span>
        </div> 
      {% endif %}
      {% endcomment %}
      {% if product.metafields.pimData.modelNumber != blank %}
      <div class="model-tag-custom">
         <label>Modèle:</label>
         <span id="model_number">{{ product.metafields.pimData.modelNumber | split:' ' | last }}</span>
         <span class="copy-btn" data-type="attribute" data-attr-name="data-clipboard-text" data-clipboard-text="{{ product.metafields.pimData.modelNumber | split:' ' | last }}">{% render 'icon-copy' %}</span>
      </div>
      {% endif %}
        
      {% if product.selected_or_first_available_variant.inventory_quantity > 0 %}
         <div class="instock-tag-custom">En Stock</div>
      {% endif %}
    </div>

      {%- if hasSalesMultiple -%}
      {% capture _soldSetsBadge %}
      <div class="soldSetsBadge RRR">vendus par lots</div>
      {% endcapture %}
      {% capture _soldSetsText %}
